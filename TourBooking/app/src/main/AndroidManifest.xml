<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TourBooking"
        tools:targetApi="31">
        <activity
            android:name=".activities.tripdetail.TripDetailActivity"
            android:exported="false"
            android:label="@string/title_activity_trip_detail"
            android:theme="@style/Theme.TourBooking" />
        <activity
            android:name=".activities.home.HomeActivity"
            android:exported="false"
            android:label="@string/title_activity_wish_list"
            android:theme="@style/Theme.TourBooking" />
        <activity
            android:name=".activities.profile.ProfileActivity"
            android:exported="false"
            android:label="@string/title_activity_profile"
            android:theme="@style/Theme.TourBooking" />
        <activity
            android:name=".activities.wishlist.WishListActivity"
            android:exported="false"
            android:label="@string/title_activity_wish_list"
            android:theme="@style/Theme.TourBooking" />
        <activity
            android:name=".activities.mytrip.MyTripActivity"
            android:exported="false"
            android:label="@string/title_activity_my_trip"
            android:theme="@style/Theme.TourBooking" />
        <activity
            android:name=".activities.MainActivity"
            android:exported="true"
            android:label="@string/title_activity_main"
            android:theme="@style/Theme.TourBooking">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>